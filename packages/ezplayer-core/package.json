{
    "name": "@ezplayer/ezplayer-core",
    "private": true,
    "version": "0.0.5",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "default": "./dist/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "test": "vitest",
        "format": "prettier --write ./src",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "uuid": "^11.0.5"
    },
    "devDependencies": {
        "tsup": "^8.1.0",
        "typescript": "^5.7.3",
        "vitest": "^2.1.8",
        "eslint": "^9.19.0",
        "typescript-eslint": "^8.21.0",
        "rimraf": "^6.0.1"
    }
}
